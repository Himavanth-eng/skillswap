<html><head><%- include('partials/head') %></head><body>
<%- include('partials/header') %>
<main class="container">
  <h2>Admin Dashboard</h2>
  <h3>Users</h3>
  <% users.forEach(u=>{ %>
    <div><%= u.name %> — <%= u.email %></div>
  <% }) %>
  <h3>Skills</h3>
  <% skills.forEach(s=>{ %>
    <div><%= s.title %> by <%= s.teacher.name %></div>
  <% }) %>
  <h3>Bookings</h3>
  <% bookings.forEach(b=>{ %>
    <div><%= b.skill.title %> — <%= b.status %></div>
  <% }) %>
</main>
<%- include('partials/footer') %></body></html>